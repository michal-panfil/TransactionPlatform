    <div class="popup-box popup-buy">
        <h5 class="text-center"> Buy instrument: </h5>

        <div class="form-group">
            <label>Ticker</label>
            <input type="text" class="form-control" id="buyTicker" />
            
        </div>

        <div class="form-group">
            <label>Price</label>
            <input type="number" step="0.1" class="form-control" id="buyPrice"/>
        </div>

        <div class="form-group">
            <label>Voumen</label>
            <input type="number" class="form-control" id="buyVolumen"/>
        </div>
        
        <div class="form-group">
            <button class="btn btn-success" onclick=SendBuyTransactionRequest()>Buy</button>
            <button class="btn btn-warning"onclick=CancelInstrumentForm()>Cancel</button>
        </div>        
    </div>
