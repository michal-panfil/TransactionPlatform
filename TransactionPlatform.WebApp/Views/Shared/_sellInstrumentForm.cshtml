    <div class="popup-box popup-sell">
        <h4 class="text-center"> Sell instrument: </h4>

        <div class="form-group">
            <label>Ticker</label>
            <input type="text" class="form-control" id="sellTicker" />

        </div>

        <div class="form-group">
            <label>Price</label>
            <input type="number" step="0.1" class="form-control" id="sellPrice" />
        </div>

        <div class="form-group">
            <label>Voumen</label>
            <input type="number" class="form-control" id="sellVolumen" />
        </div>

        <div class="form-group">
            <button class="btn btn-danger" onclick=SendSellTransactionRequest()>Sell</button>
            <button class="btn btn-warning" onclick=CancelInstrumentForm()>Cancel</button>
        </div>
    </div>
